<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>LANDING PAGE</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-aFq/bzH65dt+w6FI2ooMVUpc+21e0SRygnTpmBvdBgSdnuTN7QbdgL+OapgHtvPp" crossorigin="anonymous">
 </head>
<body>
    <%- include(`./navbar`) %></br>
    <h1></h1></br>
    <div id="carouselExampleAutoplaying" class="carousel slide" data-bs-ride="carousel">
      <div class="carousel-inner">
        <div class="carousel-item active">
          <img src="https://images.tokopedia.net/img/cache/1200/NXCtjv/2023/4/3/8c5aa3bc-15cf-4649-bb40-919900d68e2c.jpg.webp" class="d-block w-100" style="object-fit: cover;" alt="https://images.tokopedia.net/img/cache/1200/NXCtjv/2023/4/3/8c5aa3bc-15cf-4649-bb40-919900d68e2c.jpg.webp">
        </div>
        <div class="carousel-item">
          <img src="https://images.tokopedia.net/img/cache/1208/NsjrJu/2023/4/4/08608449-17cd-46d7-aa53-b3af60efa142.jpg.webp?ect=4g" class="d-block w-100" alt="https://images.tokopedia.net/img/cache/1208/NsjrJu/2023/4/4/08608449-17cd-46d7-aa53-b3af60efa142.jpg.webp?ect=4g">
        </div>
        <div class="carousel-item">
          <img src="https://images.tokopedia.net/img/cache/1208/NsjrJu/2023/4/4/c4086990-23bc-48a1-9a36-70e56235e2ef.jpg.webp?ect=4g" class="d-block w-100" alt="https://images.tokopedia.net/img/cache/1208/NsjrJu/2023/4/4/c4086990-23bc-48a1-9a36-70e56235e2ef.jpg.webp?ect=4g">
        </div>
      </div>
      <button class="carousel-control-prev" type="button" data-bs-target="#carouselExampleAutoplaying" data-bs-slide="prev">
        <span class="carousel-control-prev-icon" aria-hidden="true"></span>
        <span class="visually-hidden">Previous</span>
      </button>
      <button class="carousel-control-next" type="button" data-bs-target="#carouselExampleAutoplaying" data-bs-slide="next">
        <span class="carousel-control-next-icon" aria-hidden="true"></span>
        <span class="visually-hidden">Next</span>
      </button>
    </div>
    <!-- Products Card, FOTO NYA BELUM MUNCUL YANG DILANDING PAGE -->
    <section id="productsCard">
      <div class="container-fluid py-5">
        <div class="container">
          <div class="row">
            <% for (const key of products) {%>
            <!-- card 1 -->
            <div class="mb-3 col-md-3 col-6">
              <div class="card h-100 rounded-2 shadow-sm poductsCard">
                <div class="col">
                  <div class="inner">
                    <img src="<%= key.imageUrl %>" class="card-img-top img-fluid" alt="paket 1" style="width: auto; height: auto" />
                  </div>
                </div>
                <div class="card-body">
                  <p class="card-title fw-bold" style="font-size: 12px"><%= key.name %></p>
                  <p class="card-text" style="font-size: 12px">Stock : <%= key.stock %></p>
                  <p class="card-text" style="font-size: 12px"><%= formatPriceToEjs(key.price) %></p>
                  <a href="/products/<%= key.id %>/detail" style="text-decoration: none; color: #1ac0e4; font-size: 14px">Show detail</a>
                </div>
              </div>
            </div>
            <% } %>
          </div>
        </div>
      </div>
    </section>
    <!-- Products Card -->

    <!-- Category (Saya Pake "Products" Dulu karena "Categories" belum ada) -->
    <section class="category">
      <div class="container py-5">
        <div class="container-fluid">
          <div class="row justify-content-center">
            <h5 class="text-center pb-5">Select Products by Category</h5>
            <div class="col mx-auto">
              <% categories.forEach(data => { %>
                <a class="btn px-5 py-3 rounded-pill btn-outline-secondary" href="/products/<%= data.id %>/category" role="button"><%= data.name %></a>
              <% }) %>
            </div>
          </div>
        </div>
      </div>
    </section>
    <!--End Category  -->

    <script src="https://cdn.jsdelivr.net/npm/@popperjs/core@2.11.6/dist/umd/popper.min.js" integrity="sha384-oBqDVmMz9ATKxIep9tiCxS/Z9fNfEXiDAYTujMAeBAsjFuCZSmKbSSUnQlmh/jp3" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha2/dist/js/bootstrap.min.js" integrity="sha384-heAjqF+bCxXpCWLa6Zhcp4fu20XoNIA98ecBC1YkdXhszjoejr5y9Q77hIrv8R9i" crossorigin="anonymous"></script>
  </body>
</html>